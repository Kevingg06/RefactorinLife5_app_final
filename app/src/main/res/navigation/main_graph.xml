<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/imageFragment">

    <activity
        android:id="@+id/viewItemActivity"
        android:name="com.example.myapplication.ui.viewItem.presenter.ViewItemActivity"
        android:label="activity_view_item"
        tools:layout="@layout/activity_view_item"
         />
    <fragment
        android:id="@+id/commentFragment"
        android:name="com.example.myapplication.ui.comments.presenter.CommentFragment"
        android:label="fragment_comment"
        tools:layout="@layout/fragment_comment" />
    <fragment
        android:id="@+id/descriptionFragment"
        android:name="com.example.myapplication.ui.description.presenter.DescriptionFragment"
        android:label="fragment_description"
        tools:layout="@layout/fragment_description" />
    <fragment
        android:id="@+id/financingFragment"
        android:name="com.example.myapplication.ui.financing.presenter.FinancingFragment"
        android:label="fragment_financing"
        tools:layout="@layout/fragment_financing" />
    <fragment
        android:id="@+id/imageFragment"
        android:name="com.example.myapplication.ui.imagenes.presenter.ImageFragment"
        android:label="fragment_image"
        tools:layout="@layout/fragment_image" />
</navigation>